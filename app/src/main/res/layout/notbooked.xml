<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:background="@color/common_google_signin_btn_text_dark_default"
    tools:context="com.akhil.breuna.wid.Notbooked">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imageView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:padding="10dp"
        android:text="Today(12/12/2014)-snacks"
        android:textColor="#00008B"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="320dp"
        android:layout_height="70dp"

        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_toStartOf="@+id/textView17"
        android:layout_weight="1"
        android:background="@drawable/feedbacks"
        android:gravity="center"
        android:padding="10dp"
        android:shadowColor="#ff6cc450"
        android:shadowDx="5"
        android:shadowDy="5"
        android:shadowRadius="5"
        android:text="ALOO BUJJI"
        android:textColor="#0e2f44"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="170dp"
        android:layout_height="70dp"

        android:layout_alignBottom="@+id/textView7"
        android:layout_alignEnd="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_weight="1"
        android:background="@drawable/feedbacks"
        android:gravity="center"
        android:padding="10dp"
        android:shadowColor="#ff6cc450"
        android:shadowDx="5"
        android:shadowDy="5"
        android:shadowRadius="5"
        android:text="COFFEE"
        android:textColor="#0e2f44"
        android:textSize="20sp" />


    <TextView
        android:id="@+id/textView8"
        android:layout_width="320dp"
        android:layout_height="50dp"

        android:layout_alignEnd="@+id/textView17"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="13dp"
        android:gravity="center"
        android:padding="10dp"
        android:text="your order"
        android:textColor="#00008B"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView81"
        android:layout_width="320dp"
        android:layout_height="70dp"
        android:layout_alignEnd="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:background="@drawable/feedbacks"
        android:gravity="center"
        android:padding="10dp"
        android:text="No Order Yet"
        android:textColor="#0e2f44"
        android:textSize="20sp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="21dp"
        android:background="@drawable/log"
        android:text="Place-order"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/dfdsfsdf"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
